{
  "name": "leadflow-api",
  "version": "1.0.0",
  "description": "REST API for lead management system",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "start:dev": "cross-env NODE_ENV=development node src/app.js",
    "start:prod": "cross-env NODE_ENV=production node src/app.js",
    "dev": "nodemon src/app.js",
    "dev:development": "cross-env NODE_ENV=development nodemon src/app.js",
    "dev:prod": "cross-env NODE_ENV=production nodemon src/app.js",
    "migrate": "node src/config/syncDatabase.js",
    "migrate:dev": "cross-env NODE_ENV=development node src/config/syncDatabase.js",
    "migrate:prod": "cross-env NODE_ENV=production node src/config/syncDatabase.js",
    "seed": "node src/config/seedDatabase.js",
    "seed:dev": "cross-env NODE_ENV=development node src/config/seedDatabase.js",
    "seed:prod": "cross-env NODE_ENV=production node src/config/seedDatabase.js"
  },
  "keywords": [
    "api",
    "express",
    "sequelize",
    "postgresql"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "node-telegram-bot-api": "^0.67.0",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.35.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "nodemon": "^3.0.2"
  }
}
